x1, y1 = int(input()), int(input())
x2, y2 = int(input()), int(input())
if (y1 == y2 + 1 or y1 == y2 - 1) and x1 == x2:
    print('YES')
elif (y1 == y2 + 1 or y1 == y2 - 1 or y1 == y2) and x2 == x1 - 1:
    print('YES')
elif (y1 == y2 + 1 or y1 == y2 - 1 or y1 == y2) and x2 == x1 + 1:
    print('YES')
else:
    print('NO')
