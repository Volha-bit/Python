counter1 = 0
counter2 = 0
counter3 = 0
counter4 = 0
for i in range(int(input())):
    f = input().split()
    x = int(f[0]) 
    y = int(f[1])
    if x < 0:
        if y < 0:
            counter3 += 1
        elif y > 0:
            counter2 += 1
    elif x > 0:
        if y < 0:
            counter4 += 1
        elif y > 0:
            counter1 += 1
print('First:', counter1)
print('Second:', counter2)
print('Third:', counter3)
print('Fourth:', counter4)
